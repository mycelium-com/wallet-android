    <string name="verify_backup_dialog_title">Verify Private Key Backup</string>
    <string name="verify_backup_desc">Proper private key backup is a cornerstone when managing your Bitcoin funds.\n\nBy verifying your backup you make sure that your backup works and that you do not lose your funds if your Android device is lost or damaged.</string>
    <string name="verify_backup_scan_desc">Verify private key backup by scanning the private key QR code.\nSeveral formats are accepted, including the Mycelium encrypted paper backup.</string>
    <string name="verify_backup_clipboard_desc">Verify private key backup by taking the private key from the clipboard.</string>
    <string name="verify_backup_num_keys">You have %1$s keys that have never been verified.</string>
    <string name="verify_backup_one_key">You have 1 key that has never been verified.</string>
    <string name="verify_backup_no_such_record">The private key does not match any of your Bitcoin addresses.</string>
    <string name="verify_backup_ok">Private key backup successfully verified for the following address:\n\n%1$s</string>
    <string name="verify_backup_ignored">Backup ignored for this private key.</string>
    <string name="backup_not_verified">Backup not verified</string>
    <string name="verify_backup_title">Backup and Verify</string>
    <string name="verify_backup_message_text_1">Proper private key backup is a cornerstone when managing your Bitcoin funds.\nYou have one or more Bitcoin private keys where the backup has not been verified.\n\nPlease select the Backup option to back up your keys.</string>
    <string name="verify_backup_message_text_2">Then select the Verify Backup option and verify your backup.</string>
    <string name="verify_backup_message_non_expert">It seems that you have not backed up your private key.\n\nPlease select the Backup option to back up your private key.\n\nThen click the Verify Backup option and verify your backup.</string>
    <string name="verify_backup_message_expert">It seems that you have one or more Bitcoin private keys that have not been backed up.\n\nPlease select the Backup option to back up your keys.\n\nThen click the Verify Backup option and verify your backup.</string>
    <string name="encrypted_pdf_backup_title">Encrypted PDF Backup</string>
    <string name="encrypted_pdf_backup_description">You are about to back up your encrypted private keys to a PDF document.\nThe PDF document can safely be shared using email or with file-sharing apps you have installed, such as Dropbox. This is because your keys are encrypted.</string>
    <string name="encrypted_pdf_backup_password_description_1">This is the random password with checksum used for encrypting this backup.</string>
    <string name="encrypted_pdf_backup_password_description_2">This is your only opportunity for viewing the password for this backup.\nYour next backup will use a different password.\nWrite it down and keep it safe!</string>
    <string name="encrypted_pdf_backup_stretching">Preparing encryption key&#8230;</string>
